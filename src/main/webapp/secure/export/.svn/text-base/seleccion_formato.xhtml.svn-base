<ui:composition 
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:h="http://java.sun.com/jsf/html">
	
	<rich:panel>
		
		<h:form>
		
			<rich:toolBar >
			
				<rich:toolBarGroup>
				
					<h:panelGrid styleClass="boton">
						<h:outputLink target="_blak" value="export/pdf">
							<h:graphicImage value="/img/pdf.gif"/><br/>
						</h:outputLink>
						<h:outputLink target="_blak" value="export/pdf">
							<h:outputText value="PDF"/>
						</h:outputLink>
					</h:panelGrid>
					
					<h:panelGrid styleClass="boton">
						<h:outputLink target="_blak" value="export/xls">
							<h:graphicImage value="/img/excel.gif"/><br/>
						</h:outputLink>
						<h:outputLink target="_blak" value="export/xls">
							<h:outputText value="Excel"/>
						</h:outputLink>
					</h:panelGrid>
					
					<h:panelGrid styleClass="boton">
						<h:outputLink target="_blak" value="export/rtf">
							<h:graphicImage value="/img/txt.gif"/><br/>
						</h:outputLink>
						<h:outputLink target="_blak" value="export/rtf">
							<h:outputText value="RTF"/>
						</h:outputLink>
					</h:panelGrid>
				
				</rich:toolBarGroup>
				
				<rich:toolBarGroup location="right">
				
					<h:outputLink target="_blak" value="/webTurempleo/protected/export/pdf">
						<h:panelGrid>
							<h:graphicImage value="/img/pdf.gif"/>
							<h:outputText value="PDF"/>
						</h:panelGrid>
					</h:outputLink>
					
					<h:outputLink target="_blak" value="/protected/export/xls">
						<h:panelGrid>
							<h:graphicImage value="/img/excel.gif"/>
							<h:outputText value="Excel"/>
						</h:panelGrid>
					</h:outputLink>
					
					<h:outputLink target="_blak" value="/protected/export/txt">
						<h:panelGrid>
							<h:graphicImage value="/img/txt.gif"/>
							<h:outputText value="RTF" />
						</h:panelGrid>
					</h:outputLink>
				
				</rich:toolBarGroup>
			
			</rich:toolBar>
				
			
		
		</h:form>
		
