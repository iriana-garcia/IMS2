		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU" />

	<cache name="invoiceStateCache" eternal="false"
		maxElementsInMemory="1000" overflowToDisk="false" diskPersistent="false"
		timeToIdleSeconds="30000" timeToLiveSeconds="30000"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="skillCache" eternal="false" maxElementsInMemory="1000"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="30000"
		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU" />


	<cache name="skillPhoneCache" eternal="false"
		maxElementsInMemory="1000" overflowToDisk="false" diskPersistent="false"
		timeToIdleSeconds="30000" timeToLiveSeconds="30000"
		memoryStoreEvictionPolicy="LRU" />



	<cache name="eventCache" eternal="false" maxElementsInMemory="1000"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="30000"
		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU" />

	<cache name="categoryCache" eternal="false" maxElementsInMemory="1000"
