	<cache name="clientApplicationCache" eternal="false"
		maxElementsInMemory="1000" overflowToDisk="false" diskPersistent="false"
		timeToIdleSeconds="30000" timeToLiveSeconds="30000"
		memoryStoreEvictionPolicy="LRU" />

	<cache name="groupCache" eternal="false" maxElementsInMemory="1000"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="30000"
		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU" />

	<cache name="zipCodeCache" eternal="false" maxElementsInMemory="50000"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="30000"
		timeToLiveSeconds="30000" memoryStoreEvictionPolicy="LRU" />


</ehcache>